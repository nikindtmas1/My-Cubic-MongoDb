
        <main>
            <h1>{{title}}</h1>
            <div class="form">
                <form action="/cubics/{{result._id}}/delete" method="POST">
                    <label for="name">Name</label>
                    <input type="text" id="name" name="name" value="{{result.name}}" disabled="true" />
                    <label for="description">Description</label>
                    <textarea id="description" name="description" disabled="true" />{{result.description}}</textarea>
                    <label for="image">ImageUrl</label>
                    <input type="text" id="image" name="imageUrl" value="{{result.imageUrl}}" disabled="true" />
                    <label for="difficulty">Difficulty</label>
                    <select id="difficulty" name="difficultyLevel" value="" disabled="true" />
                    <option value="1">{{result.difficultyLevel}}</option>
                  
                    </select>
                    <input type="submit" value="Delete">
                </form>
            </div>
        </main>
    